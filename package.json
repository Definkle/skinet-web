{
  "name": "skinet-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-coverage": "ng test --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "format-lint-fix": "pnpm run format && pnpm run lint-fix",
    "nswag": "nswag run nswag.json",
    "nswag-format": "pnpm run nswag && pnpm run format-lint-fix",
    "generate-api": "nswag run nswag.json",
    "prepare": "husky install",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/skinet-web/browser/stats.json"
  },
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "dependencies": {
    "@angular/cdk": "21.0.1",
    "@angular/common": "^21.0.2",
    "@angular/compiler": "^21.0.2",
    "@angular/core": "^21.0.2",
    "@angular/forms": "^21.0.2",
    "@angular/material": "21.0.1",
    "@angular/platform-browser": "^21.0.2",
    "@angular/router": "^21.0.2",
    "@ngrx/operators": "^20.1.0",
    "@ngrx/signals": "^20.1.0",
    "@stripe/stripe-js": "^8.5.3",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.0.1",
    "@angular/animations": "^21.0.3",
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.2",
    "@eslint/js": "^9.39.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "angular-eslint": "21.0.1",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^8.0.0",
    "jsdom": "^27.2.0",
    "nswag": "^14.6.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.46.4",
    "vitest": "^4.0.14",
    "webpack-bundle-analyzer": "^5.0.1"
  }
}
